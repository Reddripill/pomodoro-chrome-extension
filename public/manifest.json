{
	"manifest_version": 3,
	"name": "pomodoro-chrome-extension",
	"description": "Base Level Extension",
	"version": "1.0",
	"action": {
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "./icons/icon16.png",
		"32": "./icons/icon32.png",
		"128": "./icons/icon128.png"
	},
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"permissions": [
		"storage"
	]
}